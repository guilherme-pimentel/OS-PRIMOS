@startuml


Django.Contrib.Auth.Group -- Django.Contrib.Auth.Permission
Django.Contrib.Auth.User -- Django.Contrib.Auth.Group
Django.Contrib.Auth.User -- Django.Contrib.Auth.Permission
Django.Contrib.Auth.User <-- "Funcionario"
Django.Contrib.Auth.User -- "Matricula"
"Funcionario" -- "Atividade"
"Funcionario" -- "Avaliação"
"Avaliação" -- "Matricula"
"Matricula" -- "Atividade"
"Matricula" -- "Fatura"


class "Funcionario" {
    - cargo: String
    - salario: Float
}

class "Atividade" {
    - nome: String
    - hora: Time
    - funcionario: funcionario
}

class "Avaliação" {
    - data:Date
    - Matricula: Matricula
    -peso:Float
    -altura:Float
    -avaliador:funcionario
}


class "Matricula" {
    -aluno:User
    -valor:Float
    - data_matricla: Date
    -atividade:Atividade
    - status: bool
}

class "Fatura" {
    -matricula: Matricula
    - valor: Float
    -data:date
    - data_vencimento: Date
}
@enduml
